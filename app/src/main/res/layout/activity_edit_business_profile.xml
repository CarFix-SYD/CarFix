<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00FFFFFF"
    android:backgroundTint="#0AFFFFFF"
    android:foregroundTint="#FFFFFF"
    >

    <ImageButton
        android:id="@+id/BusinessEditBack"
        android:layout_width="59dp"
        android:layout_height="57dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:adjustViewBounds="false"
        android:background="#00090909"
        android:backgroundTint="#00FFFFFF"
        android:baselineAlignBottom="false"
        android:cropToPadding="false"
        android:foregroundTint="#07121111"
        android:tint="#040303"
        app:layout_constraintBottom_toTopOf="@+id/HelloBusinessUser"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/SettingButton"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@android:drawable/ic_menu_revert" />

    <ImageView
        android:id="@+id/imgView"
        android:layout_width="419dp"
        android:layout_height="678dp"
        android:layout_weight="1"
        android:background="@drawable/background"
        android:contentDescription="TODO"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-8dp"
        tools:layout_editor_absoluteY="-22dp" />


    <Button
        android:id="@+id/buttonLoadPicture"
        android:layout_width="115dp"
        android:layout_height="25dp"
        android:layout_gravity="center"
        android:layout_marginTop="84dp"
        android:layout_weight="0"
        android:background="@drawable/button2"
        android:text="Upload picture"
        android:textAppearance="@style/BlueText"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.067"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/editBusiness"
        android:layout_width="112dp"
        android:layout_height="35dp"
        android:layout_marginStart="24dp"
        android:text="email"
        android:textAlignment="textStart"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/editBusinessProfileSave"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editBusinessCarsToTreat"
        app:layout_constraintVertical_bias="0.193"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/editBusinessBN"
        android:layout_width="184dp"
        android:layout_height="37dp"
        android:rotationX="-1"
        android:text="business_name"
        android:textAlignment="textStart"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.061"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonLoadPicture"
        app:layout_constraintVertical_bias="0.132" />

    <TextView
        android:id="@+id/editBusinessKind"
        android:layout_width="102dp"
        android:layout_height="47dp"
        android:text="kind"
        android:textAlignment="textStart"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/spinnerBusinessCity"
        app:layout_constraintHorizontal_bias="0.184"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonLoadPicture"
        app:layout_constraintVertical_bias="0.268" />

    <TextView
        android:id="@+id/editBusinessCity"
        android:layout_width="142dp"
        android:layout_height="45dp"
        android:layout_marginStart="8dp"
        android:text="City:"
        android:textAlignment="textStart"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/editBusinessA"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editBusinessKind"
        app:layout_constraintVertical_bias="0.823" />

    <TextView
        android:id="@+id/editBusinessA"
        android:layout_width="178dp"
        android:layout_height="42dp"
        android:text="Address:"
        android:textAlignment="textStart"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/editBusinessCarsToTreat"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.068"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.942" />

    <TextView
        android:id="@+id/editBusinessPas"
        android:layout_width="143dp"
        android:layout_height="55dp"
        android:layout_marginStart="24dp"
        android:text="phone"
        android:textAlignment="textStart"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editBusiness"
        app:layout_constraintVertical_bias="0.207" />

    <EditText
        android:id="@+id/editBusinessEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:layout_marginTop="32dp"
        android:autofillHints=""
        android:ems="10"
        android:inputType="textPersonName"
        android:text="name"
        app:layout_constraintStart_toEndOf="@+id/editBusiness"
        app:layout_constraintTop_toBottomOf="@+id/editBusinessCarsToTreat" />

    <EditText
        android:id="@+id/editBusinessName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:autofillHints=""
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/spinerBusinessKind"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/editBusinessBN"
        app:layout_constraintTop_toBottomOf="@+id/buttonLoadPicture"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/editBusinessAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:autofillHints=""
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/editBusinessCarsToTreat"
        app:layout_constraintStart_toEndOf="@+id/editBusinessA"
        app:layout_constraintTop_toBottomOf="@+id/buttonLoadPicture"
        app:layout_constraintVertical_bias="0.928" />

    <EditText
        android:id="@+id/editBusinessPhone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="76dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/editBusinessPas"
        app:layout_constraintTop_toBottomOf="@+id/editBusinessEmail"
        app:layout_constraintVertical_bias="0.0" />

    <Spinner
        android:id="@+id/spinerBusinessKind"
        android:layout_width="205dp"
        android:layout_height="49dp"
        android:layout_marginTop="136dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/editBusinessKind"
        app:layout_constraintTop_toBottomOf="@+id/buttonLoadPicture" />

    <Button
        android:id="@+id/editBusinessCarsToTreat"
        android:layout_width="128dp"
        android:layout_height="37dp"
        android:background="@drawable/button2"
        android:text="Vehicle models"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.069"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.662" />

    <Spinner
        android:id="@+id/spinnerBusinessCity"
        android:layout_width="203dp"
        android:layout_height="42dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="26dp"
        android:layout_marginBottom="76dp"
        app:layout_constraintBottom_toBottomOf="@+id/editBusinessAddress"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/editBusinessCity"
        app:layout_constraintTop_toBottomOf="@+id/spinerBusinessKind"
        app:layout_constraintVertical_bias="0.317" />

    <Button
        android:id="@+id/editBusinessProfileSave"
        android:layout_width="103dp"
        android:layout_height="48dp"
        android:layout_marginBottom="4dp"
        android:background="@drawable/button"
        android:text="Save"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="192dp"
        android:layout_height="100dp"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.671"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        tools:srcCompat="@tools:sample/avatars" />

</androidx.constraintlayout.widget.ConstraintLayout>