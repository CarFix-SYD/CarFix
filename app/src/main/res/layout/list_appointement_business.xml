<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:layout_editor_absoluteY="25dp">


    <TextView
        android:id="@+id/privateAppName"
        android:layout_width="169dp"
        android:layout_height="32dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="84dp"
        android:text="TextView"
        android:textColor="#050505"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/privateCar"
        android:layout_width="80dp"
        android:layout_height="26dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="88dp"
        android:text="TextView"
        android:textColor="#090909"
        app:layout_constraintStart_toEndOf="@+id/privateAppName"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/privateCarNumber"
        android:layout_width="80dp"
        android:layout_height="26dp"
        android:layout_marginTop="88dp"
        android:text="TextView"
        android:textColor="#050505"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.727"
        app:layout_constraintStart_toEndOf="@+id/privateCar"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/app_Date"
        android:layout_width="152dp"
        android:layout_height="30dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        android:textColor="#070707"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.101"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/setDescription"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="Description"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.772"
        app:layout_constraintStart_toEndOf="@+id/app_Date"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>